{
  "name": "demo",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@sap-cloud-sdk/connectivity": "^4.2.0",
    "@sap-cloud-sdk/http-client": "^4",
    "@sap-cloud-sdk/resilience": "^4",
    "@sap/cds": "^9",
    "express": "^4",
    "@sap/xssec": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "cds-plugin-ui5": "^0.13.0",
    "@cap-js/cds-test": "^0"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-project1": "cds watch --open project1/index.html?sap-ui-xx-viewCache=false --livereload false",
    "test": "node --test"
  },
  "private": true,
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/project1",
    "app/project1",
    "app/project1",
    "app/project1"
  ],
  "cds": {
    "requires": {
      "ZAPI_SALES_ORDER_SRV": {
        "kind": "odata-v2",
        "model": "srv/external/ZAPI_SALES_ORDER_SRV",
        "credentials": {
          "destination": "S4H",
          "path": "/sap/opu/odata/sap/API_SALES_ORDER_SRV"
        },
        "csrf": true,
        "csrfInBatch": true
      }
    }
  }
}
