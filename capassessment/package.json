{
  "name": "capassessment",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap-cloud-sdk/connectivity": "^4",
    "@sap-cloud-sdk/http-client": "^4",
    "@sap-cloud-sdk/resilience": "^4",
    "@sap/cds": "^9",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9",
    "cds-plugin-ui5": "^0.13.0"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-soproject": "cds watch --open com.ust.soproject/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "private": true,
  "cds": {
    "requires": {
      "auth": {
      "kind": "basic",
      "users": {
        "manager": {
          "password": "manager",
          "roles": [
            "SO_Manager"
          ]
        }
      }
    },
      "ZAPI_SALES_ORDER_SRV": {
        "kind": "odata-v2",
        "model": "srv/external/ZAPI_SALES_ORDER_SRV",
        "credentials": {
          "destination": "S4H",
          "path": "/sap/opu/odata/sap/API_SALES_ORDER_SRV"
        },
        "csrf": true,
        "csrfInBatch": true
      },
      "[production]": {
        "auth": "xsuaa"
      }
    }
  },
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/soproject"
  ]
}
